	DT1.Date_SK AS StartDateTime,
	DT2.Date_SK AS ActualEndTime,
	QuantityShipped, QualityRating, ProductionCost
FROM Broadcloth.dbo.ProductionBatch
INNER JOIN Broadcloth.dbo.Shipment
ON ProductionBatch.FactoryID = Shipment.FactoryID
INNER JOIN Broadcloth.dbo.Customer
ON Customer.CustomerID = Shipment.CustomerID
INNER JOIN Broadcloth.dbo.Factory
ON Factory.FactoryID = Shipment.FactoryID
INNER JOIN Broadcloth.dbo.ShipmentItem
ON Shipment.ShipmentID = ShipmentItem.ShipmentID
INNER JOIN Broadcloth.dbo.Item
ON Item.ItemID = ShipmentItem.ItemID
INNER JOIN DimProduct
ON DimProduct.Product_AK = Broadcloth.dbo.Item.ItemID
INNER JOIN DimCustomer
ON DimCustomer.Customer_AK = Broadcloth.dbo.Customer.CustomerID
INNER JOIN DimFactory
ON BroadclothDM.dbo.DimFactory.Factory_AK = Broadcloth.dbo.Factory.FactoryID 
INNER JOIN DimDate AS DT1
ON DT1.Date = ProductionBatch.StartDateTime
INNER JOIN DimDate AS DT2
ON DT2.Date = ProductionBatch.ActualEndTime</property>
